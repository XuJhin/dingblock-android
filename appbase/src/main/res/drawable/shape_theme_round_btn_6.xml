        <shape android:shape="rectangle">
            <corners android:radius="6dp" />
            <solid android:color="@color/color_ding_theme" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="6dp" />
            <solid android:color="@color/color_ding_theme_enable" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="6dp" />
            <solid android:color="@color/color_ding_theme_enable" />
        </shape>
    </item>
</selector>